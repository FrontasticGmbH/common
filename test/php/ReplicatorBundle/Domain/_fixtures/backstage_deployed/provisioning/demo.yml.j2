projects:
  - projectId: english
    name: English Catwalk
    previewUrl: https://english-demo.frontastic.io/preview
    webpackPort: 3001
    ssrPort: 8001
    configuration:
      cloudinary:
        mediaPoolName: global
    languages: ["en_GB@EUR"]
    defaultLanguage: en_GB@EUR
    projectSpecific:
      - Frontastic.Backstage.DeveloperBundle.Domain.App
      - Frontastic.Backstage.DeveloperBundle.Domain.Tastic
      - Frontastic.Backstage.StageBundle.Domain.Node
      - Frontastic.Backstage.StageBundle.Domain.Page
      - Frontastic.Backstage.StageBundle.Domain.Blueprint
      - Frontastic.Backstage.MasterBundle.Domain.PageMatcher
      - Frontastic.Backstage.MasterBundle.Domain.KitDefinition
      - Frontastic.Backstage.RedirectBundle.Domain.Redirect
      - Frontastic.Backstage.CustomAppBundle.Domain.AppData
      - Frontastic.Backstage.FacetBundle.Domain.Facet
    endpoints:
      - name: english
        url: https://english-demo.frontastic.io/api/endpoint
        environment: staging
  - projectId: swiss
    name: Swiss Catwalk
    previewUrl: https://swiss-demo.frontastic.io/preview
    webpackPort: 3002
    ssrPort: 8002
    configuration:
      contentful:
        spaceId: 2yke74xvylnq
        accessToken: 3d50038afea30f11997a73f887c4aa12d7d411d9da6825dabcc250b78623a530
        previewToken: 7391f414d8febe45caf58672ddb65ae62b2c37d525c10205669ba57c6f47bfa6
