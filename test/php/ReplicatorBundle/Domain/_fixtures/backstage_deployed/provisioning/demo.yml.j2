data:
  layout:
    breakpoints:
      - identifier: mobile
        name: Mobile # Could later be used in backstage, currently unsed
        userAgentRegexp: /(Android|webOS|iPhone|iPod|BlackBerry|Windows Phone)/i
      - identifier: tablet
        name: Tablet
        userAgentRegexp: /iPad/i
        minWidth: 768
      - identifier: desktop
        name: Desktop
        minWidth: 1280
