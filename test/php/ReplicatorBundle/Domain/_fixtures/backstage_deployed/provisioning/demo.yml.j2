  - fecl
  - pageScheduleFecl
  - rolesAndPermissions
  - machinesImprovedErrors
machineLimit: 2
configuration:
  kibana:
    url: https://kibana.frontastic.io
  github:
    url: https://github.com/FrontasticGmbH/frontastic
  deployments:
    staging:
      automatic: false
  commercetools:
    projectKey: frontastic
    clientId: abc123
    clientSecret: abc123
  contentful:
    spaceId: abc123
    accessToken: 123abc
    previewToken: 123abc
  cloudinary:
    cloudName: abc123
    apiKey: 12345
    apiSecret: 123abc
    autoTaggingService: "" # "google_tagging" "imagga_tagging" "aws_rek_tagging"
    autoTaggingMinConfidence: 0.8
  abtesting:
    engine: "kameleoon"
    clientId: "bla-frontastic-cloud"
    clientSecret: "abc123"
    siteCode: "123abc"
    mainGoal: 123456
  payment:
    adyen:
      apiKey: 'abc123'
      merchantAccount: FrontasticGmbH
      clientKey: 'test_123'
      originKeys:
        'http://english-demo.frontastic.io.local': 'pub.v2.123'
