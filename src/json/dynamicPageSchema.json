{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://frontastic.cloud/json/dynamicPageSchema",

    "title": "Dynamic Page schema",
    "description": "A Frontastic dynamic page definition",

    "type": "object",

    "properties": {
        "dynamicPageType": {
            "type": "string"
        },
        "isMultiple": {
            "type": "boolean"
        },
        "dataSourceType": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "icon": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "category": {
            "type": "string"
        }
    },
    "required": ["dynamicPageType", "isMultiple", "dataSourceType", "name"]
}
